{
  "name": "flower-miniapp",
  "version": "1.0.0",
  "private": true,
  "description": "百元香",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "cross-env CODE_ENV=dev npm run build:swan -- --watch",
    "dev:alipay": "cross-env CODE_ENV=dev npm run build:alipay -- --watch",
    "dev:tt": "cross-env CODE_ENV=dev npm run build:tt -- --watch",
    "dev:h5": "cross-env CODE_ENV=dev npm run build:h5 -- --watch",
    "dev:rn": "cross-env CODE_ENV=dev npm run build:rn -- --watch",
    "dev:qq": "cross-env CODE_ENV=dev npm run build:qq -- --watch",
    "dev:jd": "cross-env CODE_ENV=dev npm run build:jd -- --watch",
    "dev:quickapp": "cross-env CODE_ENV=dev npm run build:quickapp -- --watch",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.2.13",
    "@tarojs/react": "3.2.13",
    "@tarojs/runtime": "3.2.13",
    "@tarojs/taro": "3.2.13",
    "postcss-jsx": "^0.36.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.2.13",
    "@tarojs/webpack-runner": "3.2.13",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.2.13",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.2.13",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "stylelint": "^9.3.0",
    "typescript": "^4.1.0"
  }
}
